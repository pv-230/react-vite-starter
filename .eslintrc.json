{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": 1,
    "no-var": 0,
    "vars-on-top": 0,
    "no-plusplus": 0,
    "import/no-extraneous-dependencies": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-no-bind": 0,
    "jsx-a11y/label-has-associated-control": [1, { "assert": "either" }]
  }
}
